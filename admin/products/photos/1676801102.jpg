                var lazyLoad = lozad('.lazyimg',
                    {
                        threshold: 0.1,
                        load: function (el) {
                            el.src = el.getAttribute("data-src");
                        }
                    });
                lazyLoad.observe();
                $('img').on("error", function () {
                    var img = $(this);
                    try {
                        var src = img.attr('src');
                        if (src.indexOf('&d=') > -1 || src.indexOf('?d=') > -1) return;
                        if (src.indexOf('?') > -1) {
                            src = src + '&d=' + Date.now();
                        } else {
                            src = src + '?d=' + Date.now();
                        }
                        img.attr('src', src);
                        console.log('reload-img-' + src);
                    } catch (ex) { }
                });
            });
